
public class Prova1 {
	public static int exibir(int fi ,int cont)
	{
		int res;
		if(cont == 7)
		{
			return 1;
		}
		else
		{
		cont++;
		res = exibir(fi , cont) - fi;
		return res;
		}
		
	}
	public static void main (String args[])
	{
	int fib= 1;
	int cont = 0 ;
	exibir(fib ,cont);
	}
}
