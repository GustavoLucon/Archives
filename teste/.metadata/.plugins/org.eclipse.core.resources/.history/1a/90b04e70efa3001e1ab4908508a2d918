
public class Prova1 {
	public static int exibir(int fi ,int cont)
	{
		int res;
		if(cont == 7)
		{
			return 1;
		}
		else
		{
		cont++;
		return fi +	exibir(fi , cont);
		}
		
	}
	public static void main (String args[])
	{
	int fib= 0;
	int cont = 0 ;
	System.out.println(exibir(fib ,cont));
	}
}
