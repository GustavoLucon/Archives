
public class Prova1 {
	public static int exibir(int fi ,int fi2,int cont)
	{
		int res;
		
		if(cont == 7)
		{
			return fi;
		}
		else
		{
		cont++;
		res = fi  + exibir(fi , fi2 , cont) ;
		System.out.println(res);
		return res;
		}
		
	}
	public static void main (String args[])
	{
	int fib= 1;
	int fi2= 0;
	int cont = 0;
	exibir(fib , fi2 , cont);
	}
}
