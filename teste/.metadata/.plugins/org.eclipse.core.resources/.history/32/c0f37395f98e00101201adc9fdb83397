    // End of variables declaration
    public void jb1(String label1,String nome,int status,int altura) {
    	if(altura == 0)
    	{
    	Prepara(this.jLabel1,label1,nome,status);
    	}
    	else if( altura == 1)
    	{
    	Prepara(this.jLabel2,label1,nome,status);
    	}
    	else if( altura == 2)
    	{
    	Prepara(this.jLabel3,label1,nome,status);
    	}
    	else if( altura == 3)
    	{
    	Prepara(this.jLabel4,label1,nome,status);
    	}
    	else if( altura == 4)
    	{
    	Prepara(this.jLabel5,label1,nome,status);
    	}
    	else if( altura == 5)
    	{
    	Prepara(this.jLabel6,label1,nome,status);
    	}
    	else if( altura == 6)
    	{
    	Prepara(this.jLabel7,label1,nome,status);
    	}
    	else if( altura == 7)
    	{
    	Prepara(this.jLabel8,label1,nome,status);
    	}
    	else if( altura == 8)
    	{
    	Prepara(this.jLabel9,label1,nome,status);
    	}
    	else if( altura == 9)
    	{
    	Prepara(this.jLabel10,label1,nome,status);
    	}
    	else if( altura == 10)
    	{
    	Prepara(this.jLabel11,label1,nome,status);
    	}
    	else if( altura == 11)
    	{
    	Prepara(this.jLabel12,label1,nome,status);
    	}
    }


	public void Prepara(javax.swing.JLabel jb ,String label1,String nome,int status) {
		jb.setText("Nome: " +label1 + " IP: " + nome);
		if (status == 1)
		{
		jb.setIcon(new javax.swing.ImageIcon("C:\\Users\\Guga\\Desktop\\Nova Pasta\\ok.png"));
		}
		else
		{
		jb.setIcon(new javax.swing.ImageIcon("C:\\Users\\Guga\\Desktop\\Nova Pasta\\erro.png"));
		}
		Color c = new Color(255, 255, 240);
		jb.setBackground(c);
		jb.setVisible(true);
		jb.setEnabled(true);
		
	}
	public void EscreveTexto(String label1) 
	{
	textArea1.setBounds(10, 350, 540, 140);
	textArea1.setText(textArea1.getText() + "\n" +  label1); 
	textArea1.setCaretPosition(textArea1.getText().length()); 
	}
